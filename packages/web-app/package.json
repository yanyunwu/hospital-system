{
  "name": "web-app",
  "version": "0.0.1",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "max setup && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "max openapi",
    "playwright": "playwright install && playwright test",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none TAILWIND_MODE=watch UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "max test",
    "test:component": "max test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^0.9.4",
    "@ant-design/compatible": "^5.1.2",
    "@ant-design/icons": "^5.3.5",
    "@ant-design/pro-components": "^2.6.52",
    "@ant-design/pro-form": "^2.25.0",
    "@antv/data-set": "^0.11.8",
    "@antv/l7": "^2.20.18",
    "@antv/l7-maps": "^2.20.18",
    "@antv/l7-react": "^2.4.3",
    "@arco-design/web-react": "^2.61.1",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@umijs/route-utils": "^2.0.0",
    "add": "^2.0.6",
    "ahooks": "^2.10.14",
    "antd": "^5.14.1",
    "autoprefixer": "^9",
    "bizcharts": "^3.5.3-beta.0",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "^2.3.0",
    "dayjs": "^1.11.10",
    "gg-editor": "^2.0.2",
    "lodash": "^4.17.0",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.29.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^2.0.2",
    "query-string": "^9.0.0",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.2.0",
    "react-router": "^6.22.3",
    "socket.io-client": "^4.7.2",
    "swagger-ui-dist": "^5.11.7",
    "umi-serve": "^1.9.10",
    "wordcloud": "^1.2.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@playwright/test": "^1.17.0",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/wordcloud": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@umijs/fabric": "^2.8.0",
    "@umijs/max": "^4.1.5",
    "@umijs/openapi": "^1.3.0",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.2",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/plugins": "^4.1.5",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "gh-pages": "^3.2.0",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.0",
    "stylelint": "^13.0.0",
    "swagger-ui-react": "^3.52.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.5.0",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
